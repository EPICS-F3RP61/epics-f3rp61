record(bi, "EPSAMPLE:HEART_BEAT") {
	field(DTYP, "F3RP61")
	field(DESC, "Heart beat of sequence CPU")
	field(INP, "@E1")
	field(SCAN, ".1 second")
	field(ZNAM, "1")
	field(ONAM, "0")
}

