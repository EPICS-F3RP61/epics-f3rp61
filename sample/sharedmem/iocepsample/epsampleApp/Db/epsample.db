record(calcout, "EPSAMPLE:AI1:RANGE_MAX_CALC") {
	field(DESC, "Calculate max range")
	field(SCAN, "Passive")
	field(FLNK, "EPSAMPLE:AI1:RANGE_MIN_CALC PP NMS")
	field(INPA, "EPSAMPLE:AI1:RANGE_RAW_RB NPP NMS")
	field(CALC, "A=0?10:5")
	field(EGU, "V")
	field(PREC, "3")
	field(OUT, "EPSAMPLE:AI1:VOLTAGE.HIGH NPP NMS")
	field(DOPT, "Use CALC")
	field(OOPT, "Every Time")
}

record(calcout, "EPSAMPLE:AI1:RANGE_MIN_CALC") {
	field(DESC, "Calculate min range")
	field(SCAN, "Passive")
	field(INPA, "EPSAMPLE:AI1:RANGE_RAW_RB NPP NMS")
	field(CALC, "A=0?-10:(A=1?0:1)")
	field(EGU, "V")
	field(PREC, "3")
	field(OUT, "EPSAMPLE:AI1:VOLTAGE.LOW NPP NMS")
	field(DOPT, "Use CALC")
	field(OOPT, "Every Time")
}
