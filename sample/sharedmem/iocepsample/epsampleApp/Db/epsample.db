record(mbbo, "EPSAMPLE:AI1:RANGE_ST") {
        field(DTYP, "Raw Soft Channel")
        field(DESC, "Range of analog input 1")
        field(OMSL, "supervisory")
        field(OUT, "EPSAMPLE:AI1:RANGE_RAW_ST PP NMS")
	field(PINI, "YES")
	field(VAL, "0")
        field(SCAN, "Passive")
        field(ZRVL, "0")
        field(ONVL, "1")
        field(TWVL, "3")
        field(ZRST, "-10 +10 V")
        field(ONST, "0 +5 V")
        field(TWST, "+1 +5 V")
}

